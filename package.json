{
  "name": "book-search",
  "version": "1.0.0",
  "description": "Simple application to search books through GoodReads API",
  "main": "",
  "scripts": {
    "test": "mocha --require mocha-snapshots --require @babel/register './web/**/*.test.js'",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --require mocha-snapshots --require @babel/register './web/**/*.test.js'",
    "test:update-snapshot": "mocha --require mocha-snapshots --require @babel/register './web/**/*.test.js' --update",
    "build:server": "webpack --config webpack.server.js",
    "build:client": "webpack --config webpack.client.js",
    "build": "npm run build:server && npm run build:client",
    "start": "node build/bundle"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "css-loader": "^2.1.1",
