    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "mocha": "^6.0.2",
    "mocha-snapshots": "^4.2.0",
    "nyc": "^13.3.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
